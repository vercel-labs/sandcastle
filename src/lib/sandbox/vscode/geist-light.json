  "tokenColors": [
    {
      "scope": ["comment", "punctuation.definition.comment", "string.comment"],
      "settings": { "foreground": "#666666" }
    },
    {
      "scope": ["string", "string.template", "string.quoted"],
      "settings": { "foreground": "#297a3a" }
    },
    {
      "scope": ["constant.numeric", "constant.language", "constant.character"],
      "settings": { "foreground": "#0068d6" }
    },
    {
      "scope": ["variable", "variable.other", "variable.parameter"],
      "settings": { "foreground": "#171717" }
    },
    {
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": { "foreground": "#bd2864" }
    },
    {
      "scope": ["keyword.operator", "punctuation"],
      "settings": { "foreground": "#666666" }
    },
    {
      "scope": ["entity.name.function", "support.function", "meta.function-call"],
      "settings": { "foreground": "#0068d6" }
    },
    {
      "scope": ["entity.name.type", "entity.name.class", "support.class", "entity.name.tag"],
      "settings": { "foreground": "#0068d6" }
    },
    {
      "scope": ["entity.other.attribute-name"],
      "settings": { "foreground": "#a35200" }
    },
    {
      "scope": ["support.type.property-name", "entity.name.tag.css"],
      "settings": { "foreground": "#cb2a2f" }
    },
    {
      "scope": ["constant.other.color", "support.constant", "constant.other"],
      "settings": { "foreground": "#297a3a" }
    },
    {
      "scope": ["string.regexp"],
      "settings": { "foreground": "#a35200" }
    },
    {
      "scope": ["markup.heading", "entity.name.section"],
      "settings": { "foreground": "#171717", "fontStyle": "bold" }
    },
    {
      "scope": ["markup.bold"],
      "settings": { "fontStyle": "bold" }
    },
    {
      "scope": ["markup.italic"],
      "settings": { "fontStyle": "italic" }
    },
    {
      "scope": ["markup.inline.raw", "markup.fenced_code"],
      "settings": { "foreground": "#0068d6" }
    },
    {
      "scope": ["markup.deleted"],
      "settings": { "foreground": "#cb2a2f" }
    },
    {
      "scope": ["markup.inserted"],
      "settings": { "foreground": "#297a3a" }
    },
    {
      "scope": ["markup.changed"],
      "settings": { "foreground": "#0068d6" }
    },
    {
      "scope": ["meta.embedded", "source.groovy.embedded"],
      "settings": { "foreground": "#171717" }
    },
    {
      "scope": ["support.type.property-name.json"],
      "settings": { "foreground": "#171717" }
    },
    {
      "scope": ["entity.other.inherited-class"],
      "settings": { "foreground": "#0068d6", "fontStyle": "italic" }
    },
    {
      "scope": ["keyword.control.import", "keyword.control.export", "keyword.control.from"],
      "settings": { "foreground": "#bd2864" }
    },
    {
      "scope": ["variable.language.this", "variable.language.self"],
      "settings": { "foreground": "#bd2864", "fontStyle": "italic" }
    }
  ]
}
